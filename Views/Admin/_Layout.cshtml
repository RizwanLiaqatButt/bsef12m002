    <style>
        div.avatar {
            position: relative;
            float: left;
            margin: 5px;
        }

            div.avatar:hover img {
                opacity: 0.5;
            }

            div.avatar:hover input {
                display: block;
                background-color: transparent;
            }

            div.avatar input {
                position: absolute;
                display: none;
            }

                div.avatar input.update {
                    top: 20%;
                }


        div.image {
            position: relative;
            float: left;
        }

            div.image:hover img {
                opacity: 0.5;
            }

            div.image:hover input {
                display: block;
                background-color: transparent;
            }

            div.image input {
                position: absolute;
                display: none;
            }

                div.image input.update {
                    top: 0%;
                }
    </style>

